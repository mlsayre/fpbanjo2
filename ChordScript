wait ($strumtime)
play_note($EVENT_NOTE+7,$EVENT_VELOCITY,0,-1)
else
{main MAJOR 5-string chords}
if ($EVENT_NOTE > 67 and $EVENT_NOTE < 92)
play_note($EVENT_NOTE-5,$EVENT_VELOCITY,0,-1)
end if
wait ($strumtime)
if ($EVENT_NOTE > 61 and $EVENT_NOTE < 92)
play_note($EVENT_NOTE,$EVENT_VELOCITY,0,-1)
end if
wait ($strumtime)
if ($EVENT_NOTE + 4 > 65 and $EVENT_NOTE + 4 < 92)
play_note($EVENT_NOTE+3,$EVENT_VELOCITY,0,-1)
end if
wait ($strumtime)
if ($EVENT_NOTE+7 > 68 and $EVENT_NOTE + 7 < 92)
play_note($EVENT_NOTE + 7,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
if ($EVENT_NOTE+12 > 73 and $EVENT_NOTE + 12 < 92)
play_note($EVENT_NOTE + 12,$EVENT_VELOCITY,0,-1)
end if
end if
end if
else

{up strum major}
set_text ($direction, "Next: Down")
$strumcounter := 0
if ($EVENT_NOTE = 67)
play_note($EVENT_NOTE+7,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
play_note($EVENT_NOTE+3,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
play_note($EVENT_NOTE,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
play_note($EVENT_NOTE-5,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
play_note($EVENT_NOTE-12,$EVENT_VELOCITY,0,-1)
else
{main MAJOR 5-string chords}
if ($EVENT_NOTE+12 > 73 and $EVENT_NOTE + 12 < 92)
play_note($EVENT_NOTE + 12,$EVENT_VELOCITY,0,-1)
end if
wait ($strumtime)
if ($EVENT_NOTE+7 > 68 and $EVENT_NOTE + 7 < 92)
play_note($EVENT_NOTE + 7,$EVENT_VELOCITY,0,-1)
end if
wait ($strumtime)
if ($EVENT_NOTE + 3 > 65 and $EVENT_NOTE + 4 < 92)
play_note($EVENT_NOTE+3,$EVENT_VELOCITY,0,-1)
end if
wait ($strumtime)
if ($EVENT_NOTE > 61 and $EVENT_NOTE < 92)
play_note($EVENT_NOTE,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
if ($EVENT_NOTE > 67 and $EVENT_NOTE < 92)
play_note($EVENT_NOTE-5,$EVENT_VELOCITY,0,-1)
end if
end if
end if
end if
end if
{end of MINOR} 

