on init
make_perfview
set_key_color(38, $KEY_COLOR_YELLOW)
set_key_color(39, $KEY_COLOR_YELLOW)
set_key_color(40, $KEY_COLOR_YELLOW)
set_key_color(41, $KEY_COLOR_YELLOW)
set_key_color(42, $KEY_COLOR_YELLOW)
set_key_color(43, $KEY_COLOR_YELLOW)
set_key_color(44, $KEY_COLOR_YELLOW)
