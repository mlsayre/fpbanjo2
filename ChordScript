declare $strumtime
$strumtime := 50000
declare ui_label $timeknob_label(1,1)
set_text ($timeknob_label, "Strum Speed")
declare $timeknob_label_id
$timeknob_label_id := get_ui_id($timeknob_label)
set_control_par($timeknob_label_id,$CONTROL_PAR_TEXT_ALIGNMENT,1)
  set_control_par($timeknob_label_id,$CONTROL_PAR_POS_X,288)
  set_control_par($timeknob_label_id,$CONTROL_PAR_POS_Y,2)

declare ui_knob $timeknob(0,100,1)
set_knob_unit($timeknob, $KNOB_UNIT_PERCENT)
$timeknob := 50
declare $timeknob_id
$timeknob_id := get_ui_id($timeknob)
set_control_par($timeknob_id,$CONTROL_PAR_TEXT_ALIGNMENT,1)
  set_control_par($timeknob_id,$CONTROL_PAR_POS_X,288)
  set_control_par($timeknob_id,$CONTROL_PAR_POS_Y,22)
set_text ($timeknob, "Amount")
declare ui_label $direction(1,1)
declare $direction_id
$direction_id := get_ui_id($direction)
set_control_par($direction_id,$CONTROL_PAR_TEXT_ALIGNMENT,1)
  set_control_par($direction_id,$CONTROL_PAR_POS_X,288)
  set_control_par($direction_id,$CONTROL_PAR_POS_Y,62)
set_text ($direction, "Next: Down")
declare $strumcounter := 0

{chord type ui buttons}
declare ui_switch $major
declare $major_id
$major_id := get_ui_id($major)
set_control_par($major_id,$CONTROL_PAR_TEXT_ALIGNMENT,1)
  set_control_par($major_id,$CONTROL_PAR_POS_X,80)
  set_control_par($major_id,$CONTROL_PAR_POS_Y,12)
set_text ($major, "Major")

