ignore_event($EVENT_ID)
{Down strum)
{G major special case}
if ($strumcounter=0)
$strumcounter := 1
set_text ($direction, "Next: Up")
if ($EVENT_NOTE = 67)
play_note($EVENT_NOTE-12,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
play_note($EVENT_NOTE-5,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
play_note($EVENT_NOTE,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
play_note($EVENT_NOTE+4,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
play_note($EVENT_NOTE+11,$EVENT_VELOCITY,0,-1)
else
{main MAJOR 5-string chords}
if ($EVENT_NOTE > 67 and $EVENT_NOTE < 92)
play_note($EVENT_NOTE-5,$EVENT_VELOCITY,0,-1)
end if
wait ($strumtime)
if ($EVENT_NOTE > 61 and $EVENT_NOTE < 92)
play_note($EVENT_NOTE,$EVENT_VELOCITY,0,-1)
end if
