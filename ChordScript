end if
wait ($strumtime)
if ($EVENT_NOTE+10 > 71 and $EVENT_NOTE + 10 < 92)
play_note($EVENT_NOTE + 10,$EVENT_VELOCITY,0,-1)
end if
wait ($strumtime)
if ($EVENT_NOTE + 4 > 65 and $EVENT_NOTE + 4 < 92)
play_note($EVENT_NOTE+4,$EVENT_VELOCITY,0,-1)
end if
wait ($strumtime)
if ($EVENT_NOTE > 61 and $EVENT_NOTE < 92)
play_note($EVENT_NOTE,$EVENT_VELOCITY,0,-1)
wait ($strumtime)
